{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.4.3",
  "description": "__MSG_appDescription__",  
  "icons": {
    "16": "./icons/icon16.png",
    "19": "./icons/icon19.png",
    "32": "./icons/icon32.png",
    "38": "./icons/icon38.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },

  "background": {
    "service_worker": "html/js/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "__MSG_extensionTitle__",    "default_icon": {
    "16": "./icons/icon16.png",
    "19": "./icons/icon19.png",
    "32": "./icons/icon32.png",
    "38": "./icons/icon38.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "html/newtab.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["html/*", "images/*", "css/*", "js/*", "icons/icon128.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "storage",
    "bookmarks",
    "activeTab",
    "tabs"
  ],  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["html/js/content.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "default_locale": "zh_CN"
}
