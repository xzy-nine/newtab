{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.3.7",
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "/favicon.png",
    "32": "/favicon.png",
    "48": "/favicon.png",
    "128": "/favicon.png"
  },
  "background": {
    "service_worker": "html/js/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "__MSG_extensionTitle__",
    "default_icon": {
      "16": "/favicon.png",
      "32": "/favicon.png",
      "48": "/favicon.png",
      "128": "/favicon.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "html/newtab.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["html/*", "images/*", "css/*", "js/*", "favicon.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "storage",
    "bookmarks",
    "activeTab",
    "tabs"
  ],  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["html/js/content.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "default_locale": "zh_CN"
}
